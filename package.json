{
  "name": "runform-ai",
  "version": "1.0.0",
  "description": "AI-powered running form analysis using MediaPipe pose detection",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js --port 3000",
    "test": "node server.js --port 8080",
    "serve": "node server.js",
    "server": "node server.js",
    "mobile": "node server.js --port 8000",
    "build": "mkdir -p public && cp *.html *.js *.css *.md public/ 2>/dev/null || (if not exist public mkdir public) && copy *.html public\\ && copy *.js public\\ && copy *.css public\\",
    "build:win": "if not exist public mkdir public && copy *.html public\\ && copy *.js public\\ && copy *.css public\\",
    "build:unix": "mkdir -p public && cp *.html *.js *.css public/",
    "deploy": "npm run build:win && vercel --prod",
    "deploy:preview": "npm run build:win && vercel"
  },
  "keywords": [
    "running",
    "form-analysis",
    "pose-detection",
    "mediapipe",
    "ai",
    "sports",
    "biomechanics",
    "computer-vision",
    "vercel",
    "static-site"
  ],
  "author": "RunForm.AI Team",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/runform-ai/runform-ai.git"
  },
  "bugs": {
    "url": "https://github.com/runform-ai/runform-ai/issues"
  },
  "homepage": "https://runform-ai.vercel.app",
  "dependencies": {},
  "devDependencies": {
    "vercel": "^32.0.0"
  },
  "files": [
    "index.html",
    "app.js",
    "styles.css",
    "demo-config.js",
    "test.html",
    "server.js",
    "public/",
    "README.md",
    "TESTING.md",
    "DEPLOY.md"
  ],
  "vercel": {
    "version": 2,
    "name": "runform-ai",
    "public": true,
    "outputDirectory": "public"
  }
} 